<configuration>
  <configSections>
    <section name="eas" type="EAS.ConfigHandler,EAS.MicroKernel"/>
  </configSections>
  <eas>
    <configurations>
      <configurationItem name="Key" value="Value"/>
    </configurations>
    <objects>
      <!--日志管理-->
      <object name="Logger" assembly="EAS.MicroKernel" type="EAS.Services.TextLogger" LifestyleType="Thread" />
    </objects>
  </eas>
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
  </system.web>

</configuration>
